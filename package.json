{
  "name": "svelte-bricks",
  "description": "Simple masonry implementation without column balancing",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-bricks",
  "repository": "https://github.com/janosh/svelte-bricks",
  "license": "MIT",
  "version": "0.3.2",
  "type": "module",
  "svelte": "./dist/index.js",
  "bugs": "https://github.com/janosh/svelte-bricks/issues",
  "dependencies": {
    "svelte": "^5.27.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/package": "^2.3.11",
    "@vitest/coverage-v8": "^3.1.1",
    "jsdom": "^26.1.0",
    "mdsvex": "^0.12.3",
    "svelte-check": "^4.1.6",
    "svelte-preprocess": "^6.0.3",
    "svelte-toc": "^0.6.0",
    "svelte-zoo": "^0.4.17",
    "svelte2tsx": "^0.7.36",
    "vite": "^6.3.2",
    "vitest": "^3.1.1"
  },
  "keywords": [
    "svelte",
    "masonry",
    "pinterest-layout",
    "component"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./Masonry.svelte": {
      "types": "./dist/Masonry.svelte.d.ts",
      "svelte": "./dist/Masonry.svelte",
      "default": "./dist/Masonry.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 90
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
